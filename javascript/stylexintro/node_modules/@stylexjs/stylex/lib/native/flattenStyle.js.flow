type InlineStyle = {
  [key: string]: mixed,
};

type StylesArray<+T> = T | $ReadOnlyArray<StylesArray<T>>;

type Styles = StylesArray<InlineStyle | false | void | null>;

declare export function flattenStyle(...styles: Array<Styles>): {
  [key: string]: any,
};
